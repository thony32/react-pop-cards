{
    "name": "react-pop-cards",
    "version": "1.0.2",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.js",
    "keywords": [
        "card",
        "react",
        "useSpring",
        "tailwindcss"
    ],
    "files": [
        "dist",
        "index.d.ts",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thony32/react-pop-cards.git"
    },
    "dependencies": {
        "@react-spring/web": "^9.7.3",
        "chroma-js": "^2.4.2",
        "core-js": "^3.36.0",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-best-gradient-color-picker": "^3.0.5",
        "react-color": "^2.19.3",
        "react-dom": "^18.2.0",
        "react-hot-toast": "^2.4.1",
        "react-responsive": "^9.0.2",
        "react-scripts": "5.0.1"
    },
    "scripts": {
        "dev": "react-scripts start",
        "build": "rm -rf dist && NODE_ENV=production babel src/index.js --out-dir dist --copy-files && babel src/index.d.ts --out-dir dist --copy-files && babel src/lib/components/Card.js --out-dir dist/lib/components --copy-files && postcss src/index.css -o dist/index.css",
        "build:vercel": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.23.9",
        "@babel/core": "^7.23.9",
        "@babel/preset-env": "^7.23.9",
        "@babel/preset-react": "^7.23.3",
        "@monaco-editor/react": "^4.6.0",
        "autoprefixer": "^10.4.17",
        "daisyui": "^4.7.2",
        "postcss-cli": "^11.0.0",
        "react-countup": "^6.5.0",
        "react-typed": "^2.0.12",
        "tailwindcss": "^3.4.1"
    }
}
